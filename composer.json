{
    "name": "strictify/goodies-bundle",
    "description": "Set of utilities",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Zeljko Mitic",
            "email": "mitke013@gmail.com"
        }
    ],
    "require": {
        "php": "8.*",
        "symfony/framework-bundle": "^5.2",
        "symfony/form": "^5.2"
    },
    "require-dev": {
        "vimeo/psalm": "dev-master",
        "psalm/plugin-symfony": "dev-symfony-forms"
    },
    "repositories": [
        {
            "type": "github",
            "url": "git@github.com:strictify/psalm-plugin-symfony.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Strictify\\Goodies\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Strictify\\Goodies\\Tests\\": "tests",
            "Strictify\\Goodies\\Tests\\Fixture\\": "tests\\fixtures"
        }
    }
}
